# Docker Compose Environment Variables
# These are used by docker-compose.yml

# OpenAI API key (example fake value)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Database password (this should match POSTGRES_PASSWORD in docker-compose)
DB_PASSWORD=supersecure_db_password_123

# Django secret key (use a long random string in real setups)
SECRET_KEY=django-insecure-please-change-me-to-a-long-random-string